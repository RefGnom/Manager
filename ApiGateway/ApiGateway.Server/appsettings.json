{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HttpClientOptions": {
    "EnableFallback": true
  },
  "ReverseProxy": {
    "Routes": {
      "RecipientById": {
        "Match": {
          "Path": "/api/recipients/{recipientId}"
        },
        "ClusterId": "Recipients",
        "RateLimiterPolicy": "ManagerPolicy"
      },
      "RecipientAuthorization": {
        "Match": {
          "Path": "/api/recipients/{recipientId}/authorization"
        },
        "ClusterId": "Recipients",
        "RateLimiterPolicy": "ManagerPolicy"
      },
      "AllRecipientTimers": {
        "Match": {
          "Path": "/api/recipients/{recipientId}/timers"
        },
        "ClusterId": "Timers",
        "RateLimiterPolicy": "ManagerPolicy"
      },
      "RecipientTimerByName": {
        "Match": {
          "Path": "/api/recipients/{recipientId}/timers/{timerName}"
        },
        "ClusterId": "Timers",
        "RateLimiterPolicy": "ManagerPolicy"
      },
      "RecipientWorks": {
        "Match": {
          "Path": "/api/recipients/{recipientId}/works"
        },
        "ClusterId": "Works",
        "RateLimiterPolicy": "ManagerPolicy"
      },
      "RecipientWorkById": {
        "Match": {
          "Path": "/api/recipients/{recipientId}/works/{workId}"
        },
        "ClusterId": "Works",
        "RateLimiterPolicy": "ManagerPolicy"
      }
    },
    "Clusters": {
      "Recipients": {
        "Destinations": {
          "RecipientsApi": {
            "Address": "http://localhost:8084"
          }
        }
      },
      "Timers": {
        "Destinations": {
          "TimersApi": {
            "Address": "http://localhost:8083"
          }
        }
      },
      "Works": {
        "Destinations": {
          "WorksApi": {
            "Address": "http://localhost:8082"
          }
        }
      }
    }
  }
}